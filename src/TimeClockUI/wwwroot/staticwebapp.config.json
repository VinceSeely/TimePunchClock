{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/css/*",
      "/js/*",
      "/sample-data/*",
      "/*.{css,js,json,png,jpg,jpeg,gif,svg,ico,woff,woff2,ttf,eot,otf,webp,avif}"
    ]
  },
  "routes": [
    {
      "route": "/_framework/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/css/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/js/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "mimeTypes": {
    ".json": "application/json",
    ".wasm": "application/wasm",
    ".woff": "font/woff",
    ".woff2": "font/woff2",
    ".ttf": "font/ttf",
    ".eot": "application/vnd.ms-fontobject",
    ".otf": "font/otf"
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "Content-Security-Policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' https://login.microsoftonline.com https://*.msauth.net https://*.microsoft.com; connect-src 'self' https://login.microsoftonline.com https://*.msauth.net https://*.microsoft.com BACKEND_URL_PLACEHOLDER; img-src 'self' data: https:; font-src 'self' data:;"
  },
  "platform": {
    "apiRuntime": "dotnet:8.0"
  }
}
